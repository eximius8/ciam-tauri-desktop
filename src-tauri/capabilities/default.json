{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": true,
          "name": "sidecars/filereader",
          "sidecar": true
        }
      ]
    },
    "shell:allow-kill",
    "shell:allow-open",
    "opener:default",
    "sql:default",
    "sql:allow-execute",
    "dialog:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*"
        },
        {
          "url": "http://10.1.10.111:30801/*"
        }
      ]
    },
    "shell:default"
  ]
}