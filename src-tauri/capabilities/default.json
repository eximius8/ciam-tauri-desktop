{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "sql:allow-execute",    
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*"
        },
        {
          "url": "http://10.1.10.111:30801/*"          
        }
      ]
    }
  ]
}